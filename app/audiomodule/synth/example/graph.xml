<!-- Example simple synth patch -->
<graph>
    <processors>
        <processor x="10" y="10" name="Oscillator:0" type="Oscillator" waveform="Sin"/>
        <processor x="10" y="100" name="Envelope:0" type="Envelope" delay="200"/>
        <processor x="100" y="10" name="Gain:0" type="Gain"/>

        <processor x="100" y="10" name="subprocessor" type="Import" import="subprocessor.xml"/>
    </processors>
    <connectors>
        <connector fromProcessor="Oscillator:0" fromPort="output" toProcessor="Gain:0" toPort="input"/>
        <connector fromProcessor="Envelope:0" fromPort="output" toProcessor="Gain:0" toPort="gain"/>

        <connector fromProcessor="Input" fromPort="gate" toProcessor="Envelope:0" toPort="gate"/>
        <connector fromProcessor="Input" fromPort="trigger" toProcessor="Envelope:0" toPort="trigger"/>

        <connector fromProcessor="Gain:0" fromPort="output" toProcessor="Output" toPort="0"/>
        <connector fromProcessor="Gain:0" fromPort="output" toProcessor="Output" toPort="1"/>
    </connectors>
    <exports>
        <export type="output" name="output" fromProcessor="Gain:0" fromPort="output"/>
    </exports>
    </exports>
    <mappings>
        <mapping processor="Oscillator:0" parameter="waveform" mapping="CC:21"/>
    </mappings>
</graph>
